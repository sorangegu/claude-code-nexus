{
  "$schema": "https://raw.githubusercontent.com/cloudflare/wrangler/main/config-schema.json",
  "name": "claude-code-nexus",
  "main": "src/index.ts",
  "compatibility_date": "2024-07-29",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist/client"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "claude-code-nexus-local",
      "database_id": "local",
      "migrations_dir": "drizzle"
    }
  ],
  "vars": {
    "NODE_ENV": "development",
    "VITE_PORT": "5173",
    "APP_BASE_URL": "http://localhost:8787"
  },
  "env": {
    "production": {
      "assets": {
        "binding": "ASSETS",
        "directory": "./dist/client"
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "claude-code-nexus",
          "database_id": "e3d9b1ff-af19-4c52-8298-9a30bc0bc083",
          "migrations_dir": "drizzle"
        }
      ],
      "vars": {
        "NODE_ENV": "production",
        "VITE_PORT": "5173",
        "APP_BASE_URL": "https://code.gtca.me"
      }
    }
  }
}
